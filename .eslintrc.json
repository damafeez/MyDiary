{
  "env": {
  "node": true,
  "browser": true,
  "mocha": true
  },
  "extends": "airbnb-base",
  "rules": {
      "no-unused-vars": 0,
      "import/prefer-default-export": 0,
      "arrow-body-style": 0,
      "no-confusing-arrow": 0,
      "valid-typeof": 0,
      "prefer-destructuring": 1
  }
}